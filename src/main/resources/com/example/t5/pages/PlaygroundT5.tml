<html t:type="layout" title=""
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">
	<div class="row">
		<div class="col-xs-12 text-center">
			<h1>
				Playground
			</h1>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-12 text-center">
			<p>
			</p>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-12 text-center">
			<t:zone t:id="middlezone" class="">
				<p>TestData.uuid=${testdata.uuid}</p>
				<p>TestData.value=${testdata.value}</p>
			</t:zone>
		</div>
	</div>
	<div class="row">
		<div class="col-xs-12 text-center">
			<p>
				<p><t:eventlink event="ajax" zone="middlezone" class="btn btn-default">Change TestData</t:eventlink></p>
			</p>
		</div>
	</div>

	<t:block t:id="block">
		<p>TestData.uuid=${testdata.uuid}</p>
		<p>TestData.value=${testdata.value}</p>
	</t:block>
</html>
